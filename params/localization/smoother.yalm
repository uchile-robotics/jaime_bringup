# Mandatory parameters
speed_lim_v: 1.0
speed_lim_w: 2.0

accel_lim_v: 0.1 # maximum is actually 2.0, but we push it down to be smooth
accel_lim_w: 1.0

# Optional parameters
frequency: 20.0
decel_factor: 1.5

# Robot velocity feedback type:
#  0 - none (default)
#  1 - odometry
#  2 - end robot commands
robot_feedback: 2
